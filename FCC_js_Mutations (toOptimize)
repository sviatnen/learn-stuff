function mutation(arr) {
  
  var str1= arr[0];
  var str2 = arr[1];
  str1=str1.toLowerCase();
  str2=str2.toLowerCase();
  var arr2=str2.split('');
  var pos=[];
  var count=0;
  for (i=0; i<arr2.length; i++){
     
    pos[i]=str1.indexOf(arr2[i],0);
  
   if (pos[i]<0){ count++;} 
      }
  if (count!==0){return false;}
  else  return true;
}

mutation(["hello", "Hello"]);
